<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">

<hibernate-mapping>
	<class name="hospitalManagement.Patient" table="Patient">
		<id name="patientid">
			<generator class="assigned"></generator>
		</id>

		<property name="patientname"></property>
		<property name="patientage"></property>
		<property name="patientgender"></property>
		<property name="patientdisease"</property>

	</class>

</hibernate-mapping>  