<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">
<hibernate-mapping>
	<class name="com.itt.hospital.pojo.Patient" table="Patient">
		<id name="id">
			<generator class="increment"></generator>
		</id>
		<property name="patientName"></property>
		<property name="patientAge"></property>
		<property name="patientGender"></property>
		<property name="disease"></property>
	</class>
</hibernate-mapping>  